{
    "manifest_version": 3,
    "name": "TPulse Extensions",
    "description": "Collect metrics from browser usage",
    "version": "0.0.1",
    "action": {
        "default_icon": "clock.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "background",
        "nativeMessaging",
        "webNavigation"
    ]
}